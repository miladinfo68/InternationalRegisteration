                                    </div>
                                </div>

                            }
                            @if (Model.Student.EducationDegrees.Where(w => w.Level == (byte)Levels.Master).Count() > 0)
                            {

                                <div class="row">
                                    <div class="col-md-2 lbl">@Resources.FieldOfStudy</div>
                                    <div class="col-md-2">
